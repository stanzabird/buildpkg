    <archive_location>https://github.com/Kitware/CMake/releases/download/v3.18.4/cmake-3.18.4.tar.gz</archive_location>
    <build_commands>
      <cmd>./bootstrap {{option_prefix}}</cmd>
      <cmd>make</cmd>
      <cmd>make install</cmd>
    </build_commands>
  </package>
